{
  "compilerOptions": {
    "allowUnreachableCode": true,
    "allowSyntheticDefaultImports": true,
    "allowUnusedLabels": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "preserveConstEnums": true,
    "removeComments": false,
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "jsx": "preserve",
    "baseUrl": ".",
    "experimentalDecorators": true,
    "lib": ["dom", "es2017"],
  },
  "exclude": [
    "node_modules"
  ]
}